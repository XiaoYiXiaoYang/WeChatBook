/* pages/publish/publish.wxss */
/*image*/
.image-group{
  display: flex;
  white-space: nowrap;
  margin-top: 30rpx;
}
.image-group image{
  display: inline-block;
  width: 100%;
  margin-right: 20rpx;
}
/*process*/
.process{
  background-color:#E6E6FA;
  margin: 20rpx;
  border-collapse: 40rpx;
  border-radius: 10rpx;
}
.process_header{
  margin: 0rpx;
  padding: 35rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
}
.process_cell{
  padding-right: 10rpx;
  width: 180rpx;
}
.arrow{
  width: 60rpx;
  height: 60rpx;
  padding-right: 10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
}
.arrow image{
  width: 24rpx;
  height: 24rpx;
}
.process_icon{
  width: 160rpx;
  height: 160rpx;
}
.process image{
  width:140rpx;
  height: 140rpx;
}
.process_detail{
  font-size: 20rpx;
  color: #666;
}
.process_footer{
  background-color:#EEE8AA;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-around;
  padding: 5rpx;
  margin: 5rpx 5rpx;
  border-radius: 10rpx;
}
.process_footer_text{
  font-size:24rpx;
}
.more{
  font-size: 24rpx;
  font-weight: 300;
  border: none;
}
/*button*/
.view_buttons{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content:space-around;
}
.button-longshadow-left{
  background-color:#A5DE37;
  border-color: #A5DE37;
}
.button-longshadow-right{
  background-color: #FEAE1B;
  border-color: #FEAE1B;
}
.button-giant {
  font-size: 24px;
  height: 70px;
  width: 50%;
  line-height: 70px;
}
.button {
  color: #666;
  font-weight: 300;
  font-family: "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
  text-decoration: none;
  text-align: center;
  margin: 5px;
  border: none;
}
